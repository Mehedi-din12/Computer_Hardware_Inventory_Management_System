<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" style="-fx-background-color: #e8f8ff;" vgap="10"
          xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="sample.AdminDetailViewController">
  <children>
    <AnchorPane prefHeight="600.0" prefWidth="800.0">
      <children>
        <ImageView fx:id="profileImageView" fitHeight="268.0" fitWidth="254.0" layoutX="32.0" layoutY="127.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <VBox layoutX="337.0" layoutY="127.0" prefHeight="380.0" prefWidth="434.0">
          <children>
            <HBox prefHeight="57.0" prefWidth="434.0">
              <children>
                <Label alignment="CENTER" prefHeight="54.0" prefWidth="118.0" style="-fx-background-color: #94f2ef;"
                       text="Full Name">
                  <font>
                    <Font size="18.0"/>
                  </font>
                  <HBox.margin>
                    <Insets right="10.0"/>
                  </HBox.margin>
                </Label>
                <TextField fx:id="fullNameTextField" prefHeight="55.0" prefWidth="317.0">
                  <font>
                    <Font size="19.0"/>
                  </font>
                </TextField>
              </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="57.0" prefWidth="434.0">
              <VBox.margin>
                <Insets top="20.0"/>
              </VBox.margin>
              <children>
                <Label alignment="CENTER" prefHeight="54.0" prefWidth="116.0" style="-fx-background-color: #94f2ef;"
                       text="Contact No">
                  <font>
                    <Font size="18.0"/>
                  </font>
                  <HBox.margin>
                    <Insets right="10.0"/>
                  </HBox.margin>
                </Label>
                <TextField fx:id="contactNoTextField" prefHeight="55.0" prefWidth="317.0">
                  <font>
                    <Font size="19.0"/>
                  </font>
                </TextField>
              </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="68.0" prefHeight="57.0" prefWidth="434.0">
              <VBox.margin>
                <Insets top="20.0"/>
              </VBox.margin>
              <children>
                <Label alignment="CENTER" prefHeight="54.0" prefWidth="116.0" style="-fx-background-color: #94f2ef;"
                       text="User Name">
                  <font>
                    <Font size="18.0"/>
                  </font>
                  <HBox.margin>
                    <Insets right="10.0"/>
                  </HBox.margin>
                </Label>
                <TextField fx:id="userNameTextField" prefHeight="55.0" prefWidth="317.0">
                  <font>
                    <Font size="19.0"/>
                  </font>
                </TextField>
              </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="125.0" prefHeight="57.0" prefWidth="434.0">
              <VBox.margin>
                <Insets top="20.0"/>
              </VBox.margin>
              <children>
                <Label alignment="CENTER" prefHeight="54.0" prefWidth="116.0" style="-fx-background-color: #94f2ef;"
                       text="Password">
                  <font>
                    <Font size="18.0"/>
                  </font>
                  <HBox.margin>
                    <Insets right="10.0"/>
                  </HBox.margin>
                </Label>
                <TextField fx:id="passwordTextField" prefHeight="55.0" prefWidth="317.0">
                  <font>
                    <Font size="19.0"/>
                  </font>
                </TextField>
              </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="183.0" prefHeight="57.0" prefWidth="434.0">
              <VBox.margin>
                <Insets top="20.0"/>
              </VBox.margin>
              <children>
                <Label alignment="CENTER" prefHeight="54.0" prefWidth="116.0" style="-fx-background-color: #94f2ef;"
                       text="Joinig Date">
                  <font>
                    <Font size="18.0"/>
                  </font>
                  <HBox.margin>
                    <Insets right="10.0"/>
                  </HBox.margin>
                </Label>
                <TextField fx:id="joiningDateTextField" prefHeight="55.0" prefWidth="317.0">
                  <font>
                    <Font size="19.0"/>
                  </font>
                </TextField>
              </children>
            </HBox>
          </children>
        </VBox>
        <Button fx:id="closeButton" layoutX="507.0" layoutY="515.0" mnemonicParsing="false"
                onAction="#handleCloseButton" prefHeight="58.0" prefWidth="112.0" text="Close">
          <font>
            <Font size="18.0"/>
          </font>
        </Button>
      </children>
    </AnchorPane>
  </children>
  <columnConstraints>
    <ColumnConstraints/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints/>
  </rowConstraints>
</GridPane>
